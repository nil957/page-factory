{
  "$schema": "./registry-schema.json",
  "version": "1.0.0",
  "lastUpdated": "2026-02-08",
  "libraries": {
    "udesign": {
      "name": "UDesign",
      "package": "@ucloud-fe/react-components",
      "install": "npm install @ucloud-fe/react-components",
      "importPattern": "import { ComponentName } from '@ucloud-fe/react-components';",
      "directImportPattern": "import ComponentName from '@ucloud-fe/react-components/lib/components/ComponentName';",
      "babelPluginImport": {
        "libraryName": "@ucloud-fe/react-components",
        "camel2DashComponentName": false,
        "libraryDirectory": "lib/components"
      },
      "doc": "https://udesign.ucloud.cn",
      "repo": "https://github.com/UCloud-FE/u-design",
      "environment": "any",
      "description": "UCloud 基础 UI 组件库，可在任意 React 项目中使用"
    },
    "udesign-pro": {
      "name": "UDesign Pro",
      "package": "@ucloud/pro-components",
      "install": "npm install @ucloud/pro-components --registry=http://registry.npm.pre.ucloudadmin.com",
      "subPackages": true,
      "subPackagePattern": "@ucloud/pro-{name}",
      "importPattern": "import { ComponentName } from '@ucloud/pro-components';",
      "doc": "http://console-ops.page.ucloudadmin.com/pro-components/components",
      "environment": "any",
      "description": "基于 UDesign 的增强型组件库，可在任意 React 项目中使用，支持单独安装子包"
    },
    "common-components": {
      "name": "Console Common Components",
      "package": "common-components",
      "install": "在 .console/dependences.js 中添加 'common-components'",
      "importPattern": "import { ComponentName } from 'common-components';",
      "doc": "https://console-test03.ucloudadmin.com/components-examples/common-components/",
      "repo": "https://git.ucloudadmin.com/console/common-components",
      "environment": "console-only",
      "description": "控制台业务组件，依赖控制台框架（services, 登录态, 地域等），仅能在控制台项目中使用"
    }
  },

